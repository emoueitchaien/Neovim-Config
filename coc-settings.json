{
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": true,
    "rpc.enabled": false,
    "rpc.detailsViewing": "{workspace}",
    "rpc.lowerDetailsEditing": "{filename}",
    "explorer.sources": [{ "name": "file", "expand": true }],
    "explorer.position": "floating",
    "explorer.floating.position": "center",
    "explorer.floating.width": 100,
    "explorer.floating.border.title": "Explorer",
    "explorer.buffer.root.template": "[icon & 1] Buffers",
    "explorer.file.root.template": "[icon & 1] PROJECT ([root])",
    "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
    "explorer.file.showHiddenFiles": true,
    "explorer.file.autoReveal": false,
    "explorer.icon.enableVimDevicons": true,
    "explorer.icon.enableNerdfont": true
}
